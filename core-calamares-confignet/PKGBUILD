# Maintainer: m4teo.dev <m4teo.dev@gmail.com>

pkgname=core-calamares-confignet
_destname1="/etc"
pkgver=2.0
pkgrel=1
pkgdesc="calamares config for archlinux plasma and gnome net"
arch=('any')
url="https://github.com/Core-Linux/core-calamares-confignet"
license=('GPL3')
makedepends=('git')
depends=()
provides=("${pkgname}")
options=(!strip !emptydirs)
source=(${pkgname}::"git+${url}/${pkgname}")
sha256sums=('SKIP')

package() {
  install -dm755 ${pkgdir}${_destname1}
  cp -r ${srcdir}/${pkgname}${_destname1}/* ${pkgdir}${_destname1}
}
